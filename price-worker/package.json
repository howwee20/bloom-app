{
  "name": "price-worker",
  "version": "1.0.0",
  "description": "Bloom Price Worker - Fetches StockX prices via sneaks-api",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node index.js",
    "start:pm2": "bash scripts/pm2-start.sh",
